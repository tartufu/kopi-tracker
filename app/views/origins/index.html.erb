<h1>Origins#index</h1>
<p>Find me in app/views/origins/index.html.erb</p>


<% @origins.each do |origin| %>
<h1> <%= origin.location %> </h1>
<h1> <%= origin.phone %> </h1>
<h1> <%= link_to 'TEST', "/origins/#{origin.id}" %> </h1>
<% end %>

<% @kopis.each do |kopi| %>
	<h2> <%= kopi.name %> </h2>
	<h2> <%= kopi.roast %> </h2>
	<h2> <%= kopi.origin_id %> </h2>
<% end %>